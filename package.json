{
  "name": "RMBoilerplate",
  "version": "0.1.0",
  "author": "Matti Suur-Askola",
  "engines": {
    "node": "7.1.0"
  },
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "npm run build:client && concurrently --kill-others \"npm run run:backendServer\" \"npm run run:frontendServer\"",
    "build:client": "npm run clean && NODE_ENV=development ./node_modules/.bin/webpack --colors && npm run copy:static",
    "build:prod": "npm run clean && NODE_ENV=production webpack --progress -p  --colors && npm run copy:static",
    "copy:static": "cp -r app/static www/ && cp app/index.html www/index.html",
    "clean": "rm -rf www/*",
    "lint": "eslint app/**/*.js",
    "run:backendServer": "./node_modules/.bin/nodemon server/index.js --watch server",
    "run:frontendServer": "./node_modules/.bin/webpack-dev-server --port 8080 --hot --inline  --colors --content-base www",
    "postinstall": "npm run build:prod",
    "post-deploy": "npm install",
    "test": "jest --watch"
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-resolver": "^1.1.0",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.15.2",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.0",
    "eslint": "^3.11.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "express-ws": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "lodash": "^4.17.2",
    "lost": "^7.1.1",
    "mobx": "^2.6.3",
    "mobx-react": "^4.0.3",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "npm": "^4.0.2",
    "onvif": "^0.5.3",
    "postcss-cssnext": "^2.8.0",
    "postcss-custom-properties": "^5.0.1",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^1.1.1",
    "postcss-mixins": "^5.4.0",
    "postcss-reporter": "^2.0.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-router": "^3.0.0",
    "react-svg-inline": "^1.2.0",
    "style-loader": "^0.13.1",
    "superagent": "^3.3.1",
    "superagent-bluebird-promise": "^4.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-error-notification": "^0.1.6",
    "winston": "^2.3.0",
    "winston-daily-rotate-file": "^1.3.1"
  },
  "devDependencies": {
    "enzyme": "^2.7.0",
    "jest": "^18.1.0",
    "mobx-react-devtools": "^4.2.8",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1"
  }
}
